{
  "name": "@zap/flash-langchain-example",
  "description": "FLASH Node.js SDK Example",
  "version": "0.0.1",
  "author": "Zap Labs.",
  "license": "MIT",
  "packageManager": "pnpm@8.15.5",
  "scripts": {
    "prestart": "ts-node ./set_google_token.ts",
    "start": "NODE_OPTIONS='--no-warnings' ts-node ./chatbot.ts",
    "dev": "nodemon ./chatbot.ts",
    "lint": "eslint -c .eslintrc.json *.ts",
    "lint-fix": "eslint -c .eslintrc.json *.ts --fix",
    "format": "prettier -c .prettierrc --write \"**/*.{ts,js,cjs,json,md}\"",
    "format-check": "prettier -c .prettierrc --check \"**/*.{ts,js,cjs,json,md}\""
  },
  "dependencies": {
    "@0xzap/flash-langchain": "0.0.1",
    "@0xzap/flash": "0.0.2",
    "@langchain/core": "^0.3.19",
    "@langchain/langgraph": "^0.2.21",
    "@langchain/openai": "^0.3.14",
    "dotenv": "^16.4.5",
    "express": "^4.21.2",
    "google-auth-library": "^9.6.3",
    "googleapis": "^133.0.0",
    "zap-langchain": "0.0.4",
    "zap_tools_core": "0.0.9",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "nodemon": "^3.1.0",
    "ts-node": "^10.9.2"
  }
}